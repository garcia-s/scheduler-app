{
    "compilerOptions": {
        "types": [
            "node"
        ],
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "strict": false,
        "paths": {
            "*": [
                "node_modules/*"
            ]
        }
    }
}
